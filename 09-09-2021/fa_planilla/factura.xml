  <cac:LegalMonetaryTotal>
    <cbc:LineExtensionAmount currencyID="{{moneda}}">{{fact_op_gravada}}</cbc:LineExtensionAmount>
    <cbc:TaxInclusiveAmount currencyID="{{moneda}}">{{fact_imp_total}}</cbc:TaxInclusiveAmount>
